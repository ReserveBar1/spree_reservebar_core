<h1>New Brand</h1>

<%= form_for @brand, :url => collection_url do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :brand_owner_id %><br>
    <%= f.select(:brand_owner_id, @brand_owner_keys) %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
