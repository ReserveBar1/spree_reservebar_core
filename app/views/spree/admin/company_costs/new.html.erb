<h1>New Company Cost</h1>

<%= form_for @company_cost, :url => collection_url do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :value %><br>
    <%= f.text_field :value %>
  </p>

  <p>
    <%= f.label :percentage %><br>
    <%= f.text_field :percentage %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
